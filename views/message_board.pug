html
	head
		link(rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.98.0/css/materialize.min.css")

		link(href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet")
		link(type="text/css" rel="stylesheet" href="/styles/materialize.css" media="sreen,projection")
	body
		nav
			.nav-wrapper
				a.brand-logo.center(href='#!') Message Board
				ul.left.hide-on-med-and-down
					li
						a(href='message_board') Home
					li
						a(href='message') Post!

		h1 Welcome to our message board.

		p 
			each key in allMessages
				p Title: #{key.title}
				p Message: #{key.body}
	script(type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js")
	script(type="text/javascript" src="static/materialize.js")